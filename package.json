{
  "name": "issue-complete",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "version": "1.1.0",
  "description": "A GitHub app that labels and comments on new issues that don't have a completed task list",
  "author": "Steven Zeck <steven.zeck@gmail.com>",
  "license": "ISC",
  "repository": "https://github.com/szeck87/issue-complete.git",
  "scripts": {
    "build": "npm run lint && npm run clean && npm run compile",
    "clean": "rimraf dist",
    "compile": "tsc",
    "lint": "tslint '{src}/**/*.ts'",
    "start": "npm run build && probot run ./dist/index.js",
    "test": "npm run lint && jest"
  },
  "dependencies": {
    "probot": "^7.0.0-typescript.4",
    "probot-config": "^0.1.0"
  },
  "devDependencies": {
    "@types/jest": "^23.0.0",
    "@types/node": "^10.3.1",
    "jest": "^23.1.0",
    "rimraf": "^2.6.2",
    "smee-client": "^1.0.1",
    "standard": "^11.0.1",
    "ts-jest": "^22.4.6",
    "tslint": "^5.10.0",
    "tslint-microsoft-contrib": "^5.0.3",
    "typescript": "^2.9.1"
  },
  "engines": {
    "node": "8.x",
    "npm": ">= 4.0.0"
  },
  "standard": {
    "env": [
      "jest"
    ]
  }
}
